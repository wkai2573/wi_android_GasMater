<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_graph"
  app:startDestination="@id/mainFragment">
  <fragment
    android:id="@+id/mainFragment"
    android:name="com.wavein.gasmater.ui.main.MainFragment"
    android:label="fragment_main"
    tools:layout="@layout/fragment_main" >
    <action
      android:id="@+id/action_mainFragment_to_bleDeviceFragment"
      app:destination="@id/bleDeviceFragment" />
    <action
      android:id="@+id/action_mainFragment_to_testFragment"
      app:destination="@id/testFragment" />
  </fragment>
  <fragment
    android:id="@+id/testFragment"
    android:name="com.wavein.gasmater.ui.test.TestFragment"
    android:label="fragment_test"
    tools:layout="@layout/fragment_test" />
  <fragment
    android:id="@+id/bleDeviceFragment"
    android:name="com.wavein.gasmater.ui.bt.BleDeviceFragment"
    android:label="fragment_bt"
    tools:layout="@layout/fragment_ble" />
</navigation>