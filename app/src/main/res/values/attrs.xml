<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!-- 自訂元件參數 -->
  <declare-styleable name="Field">
    <attr name="fieldTitle" format="string" />
    <attr name="fieldValue" format="string" />
  </declare-styleable>

  <declare-styleable name="FieldAdvInput">
    <attr name="fieldAdvTitle" format="string" />
    <attr name="readLine" format="boolean" />
    <attr name="readValue" format="string" />
    <attr name="readDetailBtn" format="boolean" />
    <attr name="readChecked" format="boolean" />
    <attr name="writeLine" format="boolean" />
    <attr name="writeValue" format="string" />
    <attr name="writeDetailBtn" format="boolean" />
    <attr name="writeChecked" format="boolean" />
    <attr name="bottomDivider" format="boolean" />
  </declare-styleable>

  <declare-styleable name="FieldAdvFuseFlow">
    <attr name="fieldAdvFuseFlowTitle" format="string" />
    <attr name="fieldAdvFuseFlowReadChecked" format="boolean" />
    <attr name="fieldAdvFuseFlowWriteChecked" format="boolean" />
    <attr name="fieldAdvFuseFlowBottomDivider" format="boolean" />
  </declare-styleable>

  <declare-styleable name="ErrorTextView">
    <attr name="errorText" format="string" />
  </declare-styleable>

  <declare-styleable name="DetailOption">
    <attr name="detailOptionTitle" format="string" />
    <attr name="detailOptionEditable" format="boolean" />
  </declare-styleable>

</resources>